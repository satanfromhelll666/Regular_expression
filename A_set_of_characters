import re

txt = "The rain in ai ai a Spain falls mainly in the plain! 1"

x = re.findall("[tai]", txt) #find only "tai" char

x1 = re.findall("[^arn]",txt) # find char without "arn"

x2= re.findall("[a-n]",txt) #find char range of a to n

x3 = re.findall("[a-dA-D]",txt) #find small and capital letter 

print(x)
print(x1)
print(x2)
print(x3)

if (x):
  print("Yes, there is at least one match!")
else:
  print("No match")

